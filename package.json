{
  "name": "react-bulma-notification",
  "version": "1.1.0",
  "description": "Notification component for react and bulma.",
  "main": "build/index.js",
  "files": [
    "src/lib/",
    "build/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lxzxl/react-bulma-notification.git"
  },
  "keywords": [
    "notification notify bulma react"
  ],
  "author": "lxzxl <lixiaozheng1990@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lxzxl/react-bulma-notification/issues"
  },
  "dependencies": {
    "autoprefixer": "7.1.2",
    "bulma": "^0.6.1",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.4",
    "dotenv": "4.0.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "fs-extra": "3.0.1",
    "highlight.js": "^9.12.0",
    "html-webpack-plugin": "2.29.0",
    "jest": "^24.7.1",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.6",
    "promise": "8.0.1",
    "rc-notification": "^3.0.0",
    "react": "^16.2.0",
    "react-dev-utils": "^4.0.1",
    "react-dom": "^16.2.0",
    "source-map-loader": "^0.2.1",
    "style-loader": "0.18.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "ts-jest": "^20.0.7",
    "ts-loader": "^2.3.7",
    "tslint": "^5.7.0",
    "tslint-loader": "^3.5.3",
    "tslint-react": "^3.2.0",
    "typescript": "^2.6.2",
    "url-loader": "0.5.9",
    "webpack": "3.5.1",
    "webpack-dev-server": "2.7.1",
    "webpack-manifest-plugin": "1.2.1",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "doc": "node scripts/doc.js",
    "test": "node scripts/test.js --env=jsdom --setupTestFrameworkScriptFile=raf/polyfill",
    "prepublish": "yarn build",
    "minor": "yarn publish --new-version minor",
    "major": "yarn publish --new-version major",
    "patch": "yarn publish --new-version patch",
    "postpublish": "git describe && git push origin master --follow-tags",
    "toc": "doctoc --github --title \"# Changelog\" CHANGELOG.md"
  },
  "devDependencies": {
    "@types/highlight.js": "^9.12.2",
    "@types/jest": "^22.0.0",
    "@types/node": "^8.5.2",
    "@types/react": "^16.0.31",
    "@types/react-dom": "^16.0.3",
    "doctoc": "^1.3.0",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "raf": "^3.4.0"
  },
  "jest": {
    "mapCoverage": true,
    "collectCoverageFrom": [
      "src/lib/**/*.{js,jsx,ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/lib/**/__tests__/**/*.ts?(x)",
      "<rootDir>/src/lib/**/?(*.)(spec|test).ts?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.test.json"
      }
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
